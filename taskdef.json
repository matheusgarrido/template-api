{
  "family": "party-stories-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::195355138198:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "party-stories-container",
      "image": "<IMAGE>",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/party-stories-task",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
